{
    "author": "ColinTree <502470184@qq.com>",
    "bugs": {
        "url": "https://github.com/ColinTree/gitbook-plugin-ai2-blocks/issues"
    },
    "description": "A plugin to show blocks with gitbook",
    "engines": {
        "gitbook": ">=3.0.0"
    },
    "gitbook": {
        "property": {
            "text_call": {
                "default": "call",
                "description": "text \"call\" for methods",
                "type": "string"
            },
            "text_do": {
                "default": "do",
                "description": "text \"do\" for events",
                "type": "string"
            },
            "text_set": {
                "default": "set",
                "description": "text \"set\" for properties",
                "type": "string"
            },
            "text_when": {
                "default": "when",
                "description": "text \"when\" for events",
                "type": "string"
            },
            "margin": {
                "default": 0,
                "description": "margin of blocks, have a lower priority than specified margin parts (margin_top, ect.)",
                "type": "number"
            },
            "margin_left": {
                "default": 0,
                "description": "left margin of a block",
                "type": "number"
            },
            "margin_top": {
                "default": 0,
                "description": "top margin of a block",
                "type": "number"
            },
            "right_margin": {
                "default": 0,
                "description": "right margin of a block",
                "type": "number"
            },
            "margin_bottom": {
                "default": 0,
                "description": "bottom margin of a block",
                "type": "number"
            }
        }
    },
    "license": "Apache 2",
    "main": "index.js",
    "maintainers": [
        {
            "email": "502470184@qq.com",
            "name": "ColinTree"
        }
    ],
    "name": "gitbook-plugin-ai2-blocks",
    "readme": "README.md",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ColinTree/gitbook-plugin-ai2-blocks.git"
    },
    "version": "0.4.2"
}