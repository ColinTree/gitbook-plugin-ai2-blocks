{
    "author": "ColinTree <502470184@qq.com>",
    "bugs": {
        "url": "https://github.com/ColinTree/gitbook-plugin-ai2-blocks/issues"
    },
    "description": "A plugin to show blocks with gitbook",
    "engines": {
        "gitbook": ">=3.0.0"
    },
    "gitbook": {
        "property": {
            "text_call": {
                "default": "call",
                "description": "text \"call\" for methods",
                "type": "string"
            },
            "text_do": {
                "default": "do",
                "description": "text \"do\" for events",
                "type": "string"
            },
            "text_set": {
                "default": "set",
                "description": "text \"set\" for properties",
                "type": "string"
            },
            "text_when": {
                "default": "when",
                "description": "text \"when\" for events",
                "type": "string"
            },
            "scale_level": {
                "default": 1.0,
                "description": "scale level for all blocks",
                "type": "number"
            }
        }
    },
    "license": "Apache 2",
    "main": "index.js",
    "maintainers": [
        {
            "email": "502470184@qq.com",
            "name": "ColinTree"
        }
    ],
    "name": "gitbook-plugin-ai2-blocks",
    "readme": "README.md",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ColinTree/gitbook-plugin-ai2-blocks.git"
    },
    "version": "0.3.3"
}